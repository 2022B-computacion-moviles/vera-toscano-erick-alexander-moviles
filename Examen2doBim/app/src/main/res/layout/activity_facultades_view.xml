<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="?attr/colorOnSecondary"
    tools:context=".FacultadesView">

    <ListView
        android:id="@+id/lv_facultades"
        android:layout_width="373dp"
        android:layout_height="498dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.131" />

    <Button
        android:id="@+id/btn_estudiantes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Listar estudiantes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.822" />

    <Button
        android:id="@+id/btn_crear_facultad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Crear facultad"
        app:layout_constraintEnd_toEndOf="@+id/btn_estudiantes"
        app:layout_constraintStart_toStartOf="@+id/btn_estudiantes"
        app:layout_constraintTop_toBottomOf="@+id/btn_estudiantes" />


</androidx.constraintlayout.widget.ConstraintLayout>